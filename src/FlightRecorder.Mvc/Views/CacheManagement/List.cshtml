@using System.Collections.Generic
@using FlightRecorder.Mvc.Entities
@model List<string>

@{
    var count = (Model != null) ? Model.Count : 0;
    if (count > 0)
    {
        <table class="table">
            <tr>
                <th>Key</th>
                <th />
            </tr>
            @foreach (var key in Model!)
            {
                <tr>
                    <td valign="center">@key</td>
                    <td valign="center">
                        <a class="btn btn-lg" href="javascript:RemoveCachedData('@key')"><i class="fas fa-trash"></i></a>
                    </td>
                </tr>
            }
        </table>
    }
    else
    {
        @Html.Raw("No matching cached data items")
    }
}
